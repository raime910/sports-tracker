<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<title>@ViewBag.Title</title>
	@{ Html.RenderPartial("_Includes"); }
</head>
<body class="layout-boxed navbar-top">
	@{
		Html.RenderPartial("_NavbarWrapper");
	}
	<!-- Page container -->
	<div class="page-container">
		<!-- Page content -->
		<div class="page-content">
			@{
			Html.RenderPartial("_SidebarWrapper");
			}
			<!-- Main content -->
			<div class="content-wrapper">
				<!-- Page header -->
				@{
					Html.RenderPartial("_PageHeader");
				}
				<!-- /page header -->
				<!-- Content area -->
				<div class="content">
					<!-- Render Body -->
					<div class="row">
						<div class="col-lg-12">
							@RenderBody()
						</div>
					</div>
					<!-- /render body -->
					@{
						Html.RenderPartial("_Footer");
					}
				</div>
				<!-- /content area -->
			</div>
			<!-- /main content -->
		</div>
		<!-- /page content -->
	</div>
	<!-- /page container -->
</body>
</html>